{
  "name": "enhanced-ml-trading-dashboard",
  "version": "3.0.0",
  "description": "Production-ready ML Trading Dashboard with NinjaTrader integration",
  "main": "ml-server.js",
  "scripts": {
    "start": "node ml-server.js",
    "dev": "nodemon ml-server.js",
    "dev:dashboard": "next dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:dashboard\"",
    "build": "next build",
    "build:all": "npm install && npm run lint && next build",
    "start:dashboard": "next start",
    "start:all": "concurrently \"npm run start\" \"npm run start:dashboard\"",
    "test": "jest",
    "lint": "eslint *.js && next lint",
    "docker:build": "docker build -t ml-trading-dashboard .",
    "docker:run": "docker run -p 8080:8080 -p 9999:9999 -p 3000:3000 ml-trading-dashboard",
    "health-check": "curl -f http://localhost:8080/health || exit 1"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-cpu": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "bull": "^4.16.5",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "fs-extra": "^11.3.0",
    "helmet": "^7.2.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "lru-cache": "^10.4.3",
    "ml-matrix": "^6.12.1",
    "ml-regression-multivariate-linear": "^2.0.4",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "msgpack-lite": "^0.1.26",
    "next": "^14.0.0",
    "node-fetch": "^3.3.2",
    "opossum": "^7.1.0",
    "pg": "^8.11.3",
    "prom-client": "^15.1.3",
    "random-forest-classifier": "^0.6.0",
    "rate-limiter-flexible": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redis": "^4.7.1",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.0",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.6",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/ml-trading-dashboard.git"
