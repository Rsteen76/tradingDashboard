{
    "name": "ml-trading-server",
    "version": "1.0.0",
    "description": "ML-powered trading server with advanced pattern recognition and adaptive learning",
    "main": "src/server.js",
    "scripts": {
        "start": "node src/server.js",
        "dev": "nodemon src/server.js",
        "test": "jest --detectOpenHandles",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/**/*.js test/**/*.js",
        "format": "prettier --write 'src/**/*.js' 'test/**/*.js'"
    },
    "dependencies": {
        "@tensorflow/tfjs": "^4.17.0",
        "@tensorflow/tfjs-node": "^4.22.0",
        "dotenv": "^16.4.1",
        "express": "^4.18.2",
        "lru-cache": "^10.2.0",
        "moment": "^2.30.1",
        "node-fetch": "^2.7.0",
        "technicalindicators": "^3.1.0",
        "uuid": "^9.0.1",
        "winston": "^3.11.0",
        "ws": "^8.16.0",
        "socket.io": "^4.7.5"
    },
    "devDependencies": {
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.3",
        "prettier": "^3.2.5",
        "supertest": "^6.3.4"
    },
    "jest": {
        "testEnvironment": "node",
        "testTimeout": 10000,
        "setupFilesAfterEnv": [
            "./test/setup.js"
        ],
        "collectCoverageFrom": [
            "src/**/*.js",
            "!src/server.js"
        ],
        "coverageReporters": [
            "text",
            "lcov",
            "html"
        ],
        "verbose": true,
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/coverage/"
        ]
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/ml-trading-server"
    },
    "keywords": [
        "machine-learning",
        "trading",
        "tensorflow",
        "pattern-recognition",
        "adaptive-learning",
        "real-time"
    ],
    "author": "",
    "license": "MIT"
}
